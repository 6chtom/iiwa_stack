<?xml version="1.0"?>

<robot name="lbr_iiwa" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find lbr_iiwa_description)/urdf/lbr_iiwa.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find lbr_iiwa_description)/urdf/materials.xacro" />
  <!-- Import Utilities -->
  <xacro:include filename="$(find lbr_iiwa_description)/urdf/utilities.xacro"/>

  <!-- some constants -->
  <xacro:property name="base_mass" value="2.0"/>
  <xacro:property name="link_mass" value="2.0"/>
  <xacro:property name="ball_link_mass" value="2.0"/>
  <xacro:property name="end_link_mass" value="2.0"/>
  <xacro:property name="safety_controller_k_pos" value="100" />
  <xacro:property name="safety_controller_k_vel" value="2" />
  <xacro:property name="joint_damping" value="0.1" />
  <xacro:property name="velocity_scale_factor" value="1"/>

  <!-- right is either 1 (for right arm) or -1 (for left arm) -->
  <xacro:macro name="lbr_iiwa" params="parent name right *origin">

    <!--joint between {parent} and arm_0_link-->
    <joint name="${parent}_base_joint" type="fixed">
      <insert_block name="origin"/>
      <parent link="${parent}"/>
      <child link="${name}_0_link"/>
    </joint>

    <link name="${name}_0_link">
      <inertial>
        <mass value="${base_mass}"/>
        <origin xyz="0 0 0.055" rpy="0 0 0"/>
        <cylinder_inertia_def radius="0.06" length="0.11"
                              mass="${base_mass}"/>
      </inertial>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_0.stl"/>
        </geometry>
        <material name="Black"/>
      </visual>

      <collision>
           <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_0.stl"/>
        </geometry>
        <material name="Black"/>
      </collision>
    </link>

<!-- joint between arm_0_link and arm_1_link -->
    <joint name="${name}_1_joint" type="revolute">
		<parent link="${name}_0_link"/>
		<child link="${name}_1_link"/>
      <origin xyz="0 0 0.11" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="${-170 * M_PI / 180}" upper="${170 * M_PI / 180}"
             effort="204" velocity="${velocity_scale_factor * 110 * M_PI / 180}" />
   <!--   <safety_controller soft_lower_limit="${-168 * M_PI / 180}"
                         soft_upper_limit="${168 * M_PI / 180}"
                         k_position="${safety_controller_k_pos}"
                         k_velocity="${safety_controller_k_vel}"/> -->
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${name}_1_link">
      <inertial>
        <mass value="${link_mass}"/>
        <origin rpy="0 0 0" xyz="0 0 0.130"/>
        <cuboid_inertia_def length="0.12" width="0.06" height="0.260"
                            mass="${link_mass}"/>
      </inertial>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_1.stl"/>
        </geometry>
        <material name="Orange"/>
      </visual>

		<collision>
        <origin xyz="0 0 0" rpy="0 0 ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_1.stl"/>
        </geometry>
        <material name="Orange"/>
		</collision>
    </link>

  <!--  <transmission name="${name}_arm_0_trans" type="pr2_mechanism_model/SimpleTransmission">
      <actuator name="${name}_arm_0_motor"/>
      <joint name="${name}_arm_0_joint"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission> -->

        <!-- joint between arm_1_link and arm_2_link -->
    <joint name="${name}_2_joint" type="revolute">
		<parent link="${name}_1_link"/>
		<child link="${name}_2_link"/>
      <origin xyz="0 0 0.20" rpy="0 0 0"/>
      <axis xyz="0 -1 0"/>
      <limit lower="${-120 * M_PI / 180}" upper="${120 * M_PI / 180}"
          effort="306" velocity="${arm_velocity_scale_factor * 110 * M_PI / 180}" />  
     <!-- <safety_controller soft_lower_limit="${-118 * M_PI / 180}"
                         soft_upper_limit="${118 * M_PI / 180}"
                         k_position="${safety_controller_k_pos}"
                         k_velocity="${safety_controller_k_vel}"/> -->
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${name}_2_link">
      <inertial>
        <mass value="${link_mass}"/>
        <origin rpy="0 0 0" xyz="0 0.06 ${0.130 - 0.06}"/>
        <cuboid_inertia length="0.12" width="0.06" height="0.260"
                            mass="${link_mass}"/>
      </inertial>

      <visual>
        <origin xyz="0 0 0.2" rpy="${M_PI} 0 ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_2.stl"/>
        </geometry>
        <material name="Orange"/>
      </visual>

      <collision>
        <origin xyz="0 0 0.2" rpy="${M_PI} 0 ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_2.stl"/>
        </geometry>
        <material name="Orange"/>
      </collision>
    </link>

  <!--  <transmission name="${name}_arm_1_trans" type="pr2_mechanism_model/SimpleTransmission">
      <actuator name="${name}_arm_1_motor"/>
      <joint name="${name}_arm_1_joint"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission> -->

  <!-- joint between arm_2_link and arm_3_link -->
    <joint name="${name}_3_joint" type="revolute">
		<parent link="${name}_2_link"/>
		<child link="${name}_3_link"/>
      <origin xyz="0 0 0.20" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="${-170 * M_PI / 180}" upper="${170 * M_PI / 180}"
             effort="204" velocity="${arm_velocity_scale_factor * 130 * M_PI / 180}" />
      <!--<safety_controller soft_lower_limit="${-168 * M_PI / 180}"
                         soft_upper_limit="${168 * M_PI / 180}"
                         k_position="${safety_controller_k_pos}"
                         k_velocity="${safety_controller_k_vel}"/> -->
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${name}_3_link">
      <inertial>
        <mass value="${link_mass}"/>
        <origin rpy="0 0 0" xyz="0 0.06 0.130"/>
        <cuboid_inertia_def length="0.12" width="0.06" height="0.260"
                            mass="${link_mass}"/>
      </inertial>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_3.stl"/>
        </geometry>
        <material name="Orange"/>
      </visual>

      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_3.stl"/>
        </geometry>
        <material name="Orange"/>
      </collision>
    </link>

  <!--  <transmission name="${name}_arm_2_trans" type="pr2_mechanism_model/SimpleTransmission">
      <actuator name="${name}_arm_2_motor"/>
      <joint name="${name}_arm_2_joint"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission> -->

        <!-- joint between arm_3_link and arm_4_link -->
    <joint name="${name}_4_joint" type="revolute">
		<parent link="${name}_3_link"/>
		<child link="${name}_4_link"/>
      <origin xyz="0 0 0.20" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <limit lower="${-120 * M_PI / 180}" upper="${120 * M_PI / 180}"
             effort="306" velocity="${arm_velocity_scale_factor * 130 * M_PI / 180}" />
     <!-- <safety_controller soft_lower_limit="${-118 * M_PI / 180}"
                         soft_upper_limit="${118 * M_PI / 180}"
                         k_position="${safety_controller_k_pos}"
                         k_velocity="${safety_controller_k_vel}"/>-->
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${name}_4_link">
      <inertial>
        <mass value="${link_mass}"/>
        <origin rpy="0 0 0" xyz="0 -0.06 ${0.130 - 0.06}"/>
        <cuboid_inertia length="0.12" width="0.06" height="0.2600"
                            mass="${link_mass}"/>
      </inertial>

      <visual>
        <origin xyz="0 0 0.2" rpy="0 ${M_PI} ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_4.stl"/>
        </geometry>
        <material name="Orange"/>
      </visual>

      <collision>
       <origin xyz="0 0 0.2" rpy="0 ${M_PI} ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_4.stl"/>
        </geometry>
        <material name="Orange"/>
      </collision>
    </link>

  <!--  <transmission name="${name}_arm_3_trans" type="pr2_mechanism_model/SimpleTransmission">
      <actuator name="${name}_arm_3_motor"/>
      <joint name="${name}_arm_3_joint"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission> -->

        <!-- joint between arm_4_link and arm_5_link -->
    <joint name="${name}_5_joint" type="revolute">
		<parent link="${name}_4_link"/>
		<child link="${name}_5_link"/>
      <origin xyz="0 0 0.20" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="${-170 * M_PI / 180}" upper="${170 * M_PI / 180}"
             effort="204" velocity="${arm_velocity_scale_factor * 130 * M_PI / 180}" />
     <!-- <safety_controller soft_lower_limit="${-168 * M_PI / 180}"
                         soft_upper_limit="${168 * M_PI / 180}"
                         k_position="${safety_controller_k_pos}"
                         k_velocity="${safety_controller_k_vel}"/> -->
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${name}_5_link">
      <inertial>
        <mass value="${link_mass}"/>
        <origin rpy="0 0 0" xyz="0 0 0.124"/>
        <cuboid_inertia length="0.12" width="0.06" height="0.248"
                            mass="${link_mass}"/>
      </inertial>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_5.stl"/>
        </geometry>
        <material name="Orange"/>
      </visual>

      <collision>
        <origin xyz="0 0 0" rpy="0 0 ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_5.stl"/>
        </geometry>
        <material name="Orange"/>
      </collision>
    </link>

  <!-- <transmission name="${name}_arm_4_trans" type="pr2_mechanism_model/SimpleTransmission">
      <actuator name="${name}_arm_4_motor"/>
      <joint name="${name}_arm_4_joint"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission> -->

  <!-- joint between arm_5_link and arm_6_link -->
    <joint name="${name}_6_joint" type="revolute">
  		<parent link="${name}_5_link"/>
		<child link="${name}_6_link"/>
      <origin xyz="0 0 0.19" rpy="0 0 0"/>
      <axis xyz="0 -1 0"/>
      <limit lower="${-120 * M_PI / 180}" upper="${120 * M_PI / 180}"
             effort="306" velocity="${arm_velocity_scale_factor * 180 * M_PI / 180}" />
    <!--  <safety_controller soft_lower_limit="${-118 * M_PI / 180}"
                         soft_upper_limit="${118 * M_PI / 180}"
                         k_position="${safety_controller_k_pos}"
                         k_velocity="${safety_controller_k_vel}"/> -->
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${name}_6_link">
      <inertial>
        <mass value="${ball_link_mass}"/>
        <origin rpy="0 0 0" xyz="0 0 0.0625"/>
        <cuboid_inertia length="0.125" width="0.125" height="0.125"
                            mass="${ball_link_mass}"/>
      </inertial>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_6.stl"/>
        </geometry>
        <material name="Orange"/>
      </visual>

      <collision>
       <origin xyz="0 0 0" rpy="0 0 ${M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_6.stl"/>
        </geometry>
        <material name="Orange"/>
      </collision>
    </link>

  <!--  <transmission name="${name}_arm_5_trans" type="pr2_mechanism_model/SimpleTransmission">
      <actuator name="${name}_arm_5_motor"/>
      <joint name="${name}_arm_5_joint"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission> -->

<!-- joint between arm_6_link and arm_7_link -->
    <joint name="${name}_7_joint" type="revolute">
		<parent link="${name}_6_link"/>
		<child link="${name}_7_link"/>
      <origin xyz="0 0 0.078" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit lower="${-170 * M_PI / 180}" upper="${170 * M_PI / 180}"
             effort="204" velocity="${arm_velocity_scale_factor * 180 * M_PI / 180}" />
      <!-- <safety_controller soft_lower_limit="${-168 * M_PI / 180}"
                         soft_upper_limit="${168 * M_PI / 180}"
                         k_position="${safety_controller_k_pos}"
                         k_velocity="${safety_controller_k_vel}"/> -->
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${name}_7_link">
      <inertial>
        <mass value="${end_link_mass}"/>
        <origin xyz="0 0 0"/>
        <cuboid_inertia_def length="1" width="1" height="1"
                            mass="${end_link_mass}"/>
      </inertial>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 ${right * -1/4 * M_PI  +  M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_7.stl"/>
        </geometry>
        <material name="Grey"/>
      </visual>

      <collision>
        <origin xyz="0 0 0" rpy="0 0 ${right * -1/4 * M_PI  +  M_PI}"/>
        <geometry>
          <mesh filename="package://lbr_iiwa_description/meshes/link_7.stl"/>
        </geometry>
        <material name="Grey"/>
      </collision>
    </link>
    
  <!--  <transmission name="${name}_arm_6_trans" type="pr2_mechanism_model/SimpleTransmission">
      <actuator name="${name}_arm_6_motor"/>
      <joint name="${name}_arm_6_joint"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission> -->
    
  </xacro:macro>

</robot>
